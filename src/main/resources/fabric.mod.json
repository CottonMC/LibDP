    ]
  },
  "mixins": [
    "mixins.libdp.json"
  ],
  "accessWidener": "libdp.accesswidener",
  "depends": {
    "fabricloader": ">=0.4.0"
  },
  "custom": {
    "badges": [ "library" ]
  }
