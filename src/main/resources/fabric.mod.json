{
  "schemaVersion": 1,
  "id": "libdp",
  "version": "$version",

  "name": "Lib Data Potential",
  "icon": "assets/libdp/icon.png",
  "description": "Unlocking the true potential of data packs",
  "licence": "MIT",
  "contact": {
    "sources": "https://github.com/CottonMC/LibDP"
  },
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.cottonmc.libdp.LibDP"
    ],
